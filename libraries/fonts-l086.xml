<?xml version="1.0" encoding="utf-8"?>
<!-- ?xml-stylesheet href="races.xsl" type="text/xsl" ? -->
<fonts>
  <!-- shared font definitions -->
  <font name="jfont">
    <fontsize size="32" yoffset="0" />
  </font>
  <font name="csfont">
    <fontsize size="32" yoffset="0" />
  </font>
  <font name="ctfont">
    <fontsize size="32" yoffset="0" />
  </font>
  <font name="kofont">
    <fontsize size="32" yoffset="0" />
  </font>
  <!-- language specific font definitions -->
  <font name="csfont Bold">
    <fontsize size="32" yoffset="0" />
  </font>
  <font name="Stars">
    <fontsize size="32" yoffset="0" />
  </font>
  <font name="DejaVu Sans Mono Outlined">
    <fontsize size="8" yoffset="0" />
  </font>
  <alias name="large" ref="csfont" />
  <alias name="default" ref="csfont" />
  <alias name="holomap" ref="csfont" material="csfont outlined" />
  <alias name="holomap fixed" ref="csfont" material="csfont outlined" fixedwidth="true" />
  <alias name="holomap bold" ref="csfont Bold" material="csfont outlined" />
  <alias name="holomap bold fixed" ref="csfont Bold" material="csfont outlined" fixedwidth="true" />
  <alias name="Zekton" ref="csfont" />
  <alias name="Zekton Bold" ref="csfont Bold" />
  <alias name="Zekton fixed" ref="csfont" fixedwidth="true" />
  <alias name="Zekton Bold fixed" ref="csfont Bold" fixedwidth="true" />
  <alias name="Zekton Outlined" ref="csfont" material="csfont outlined" />
  <alias name="Zekton Bold Outlined" ref="csfont Bold" material="csfont bold outlined" />
  <alias name="debug" ref="DejaVu Sans Mono Outlined" />
</fonts>
