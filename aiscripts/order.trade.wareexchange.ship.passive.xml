<?xml version="1.0" encoding="utf-8"?>
<aiscript name="order.trade.wareexchange.ship.passive" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd">
  <order id="TradeExchangePassive" name="{1041, 121}" description="{1041, 122}" category="internal">
    <params>
      <param name="tradedeal" type="trade" text="{1041, 10130}" comment="Trade"/>
      <param name="debugchance" type="bool" default="0" advanced="true" text="{1041, 10086}" comment="Print debug output">
        <input_param name="truevalue" value="100"/>
      </param>
    </params>
    <requires>
      <match shiptype="shiptype.lasertower" negate="true"/>
    </requires>
    <location object="$tradedeal.exchangepartner.{this.object}" condition="$tradedeal.exists"/>
  </order>
  <attention min="unknown">
    <actions>
      <run_script name="'order.trade.wareexchange.ship'">
        <param name="tradedeal" value="$tradedeal"/>
        <param name="debugchance" value="$debugchance"/>
      </run_script>
    </actions>
  </attention>
</aiscript>
